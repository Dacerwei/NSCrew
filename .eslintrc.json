{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6
	},
	"plugins": [
		"react",
		"jsx-ally",
		"import"
	],
	"env": {
		"es6":true,
		"browser": true,
		"node": true
	},
	"extends": "airbnb",
	"rules": {
		"no-console": 0,
		"semi": ["errr", "always"],
		"quotes": ["error", "double"],
		"arrow-body-style": ["error", "always"]
	}
}